# JTERATOR PIPELINE DESCRIPTOR FILE
# =================================
# 
# The working directory is by default the directory where the .pipe file 
# is located. So you can provide relative paths in your pipeline description.
#
# To run modules individually use the following command:
#   $ cat [handles_filename] | [module_filename]


# Define your project:
project:

  name: test
  description: example pipeline for testing purposes
  version: 0.0.1


# Define iteration procedure:
jteration:

    folder: rawdata/TIFF
    pattern: .*C01\.(png|tiff?)


# Define modules (.jt executable files) and handles (.handles YAML files):
pipeline:

  - name: PrimeRawData
    module: modules/PrimeRawData.jt
    handles: handles/PrimeRawData.handles

  - name: IllumCorr
    module: modules/IllumCorr.jt
    handles: handles/IllumCorr.handles

  - name: IndentifyPrimary
    module: modules/IdentifyPrimary.jt
    handles: handles/IdentifyPrimary.handles

  - name: MeasurePrimary
    module: modules/MeasurePrimary.jt
    handles: handles/MeasurePrimary.handles

  - name: Plot
    module: modules/Plot.jt
    handles: handles/Plot.handles

  - name: Whatever
    module: modules/Whatever.jt
    handles: handles/Whatever.handles
