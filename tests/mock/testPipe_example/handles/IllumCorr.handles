# full path to HDF5 file
hdf5_filename: tmp/test.tmp

# module input arguments
input:

    OrigImage: 
        hdf5_location: /PrimeRawData/OrigImage
        class: double
        attributes: positive

    StatsMeanImage: 
        hdf5_location: /PrimeRawData/StatsMeanImage
        class: double
        attributes: positive

    StatsStdImage: 
        hdf5_location: /PrimeRawData/StatsStdImage
        class: double
        attributes: positive

# module output arguments
output:

    CorrImage:
        hdf5_location: /IllumCorr/CorrImage
        class: double
        attributes: positive
