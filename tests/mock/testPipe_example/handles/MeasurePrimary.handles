# full path to HDF5 file
hdf5_filename: tmp/test.tmp

# module input arguments
input:

    Nuclei: 
        hdf5_location: /IdentifyPrimary/Nuclei
        class: ndarray

    Test_integer:
        parameter: 4
        class: int

    Test_string:
        parameter: hello
        class: str

    Test_matrix:
        parameter: [0, 1]
        class: list

# module output arguments
output:

    Measurements:
        hdf5_location: /MeasurePrimary/Measurements
        class: float64

    LabeledSegmentationImage:
        hdf5_location: /MeasurePrimary/LabeledSegmentationImage
