# full path to HDF5 file
hdf5_filename: tmp/test.tmp

# module input arguments
input:

    CorrImage: 
        hdf5_location: /IllumCorr/CorrImage
        class: double
        attributes: positive

    ThresholdMethod:
        parameter: Otsu Global
        class: char

    ThresholdCorrection:
        parameter: 1
        class: double

    ThresholdRange:
        parameter: [0, 1]
        class: double

    pObject:
        parameter: 10%
        class: char

    saveFigure:
        parameter: Yes
        class: logical

# module output arguments
output:

    Nuclei:
        hdf5_location: /IdentifyPrimary/Nuclei
        class: double
        attributes: positive

