# module input arguments
input:

    DapiFilename:
        hdf5_location: /DAPI

    Antibody1Filename:
        hdf5_location: /Antibody1

# module output arguments
output:

    DapiImage:
        hdf5_location: /PrimeRawData/DapiImage
        class: ndarray

    Antibody1Image:
        hdf5_location: /PrimeRawData/Antibody1Image
        class: ndarray
